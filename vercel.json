{
    "version":2,
    "builds":[
        {   "src":"complaint_portal/wsgi.py",
            "use":"@vercel/python",
            "config":{"maxLambdaSize":"15mb","runtime":"python3.10"}
            
        },
        {   "src":"build_files.sh",
            "use":"@vercel/static_build",
            "config":{"distDir":"staticfiles_build","runtime":"python3.10"}
            
        }
    ],
    "routes":[
        {
            "src":"/static/(.*)",
            "dest":"/static/$1"

        },
        {
            "src":"/(.*)",
            "dest":"complaint_portal/wsgi.py"

        }
    ]
}